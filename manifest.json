{
    "manifest_version": 2,
    "name": "Weighted and Final Grade Calculator",
    "version": "1.0",
    "description": "Calculate your weighted and final grades easily and efficiently",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "permissions": [
        "storage"
    ]
}